# --- Primary workflow ---

LeadGeneration -> LeadStorage : "Capture leads"
Website -> LeadStorage : "Inbound forms"

LeadStorage -> LeadComms : "Qualified leads"
LeadComms -> DemoScheduler : "CTA: Book demo"

DemoScheduler -> Demo : "Scheduled session"
Demo -> OnboardingScheduler : "Client interest / yes"

OnboardingScheduler -> Onboarding : "Start onboarding"
Onboarding -> ClientAnalytics : "Activate service"

ClientAnalytics -> ClientUpkeep : "Usage & performance data"
ClientUpkeep -> LeadComms : "Upsells / expansions"

# --- Feedback loops ---

ClientAnalytics -> LeadStorage : "Conversion data"
LeadStorage -> LeadGeneration : "Refined targeting"
